<IP InstanceName="GPIO" Name="IO" PackName="stm32_io_F1_Cube">
    <SubFamily Name="STM32F103">
        <Configuration McuName="STM32F103C8T6">
            <Mode Name="输出电平" Widgets="QComboBox">
                <Table Key="低" Value="GPIO_PIN_RESET"/>
                <Table Key="高" Value="GPIO_PIN_SET"/>
            </Mode>
            <Mode Name="模式" Widgets="QComboBox">
                <Table Key="浮空输入" Value="GPIO_MODE_INPUT"/>
                <Table Key="推挽输出" Value="GPIO_MODE_OUTPUT_PP"/>
                <Table Key="开漏输出" Value="GPIO_MODE_OUTPUT_OD"/>
                <Table Key="复用推挽" Value="GPIO_MODE_AF_PP"/>
                <Table Key="复用开漏" Value="GPIO_MODE_AF_OD"/>  
                <Table Key="复用输入" Value="GPIO_MODE_AF_INPUT"/>
                <Table Key="上升沿中断" Value="GPIO_MODE_IT_RISING"/>
                <Table Key="下降沿中断" Value="GPIO_MODE_IT_FALLING"/>
                <Table Key="上升沿下降沿中断" Value="GPIO_MODE_IT_RISING_FALLING"/>
                <Table Key="上升沿事件" Value="GPIO_MODE_EVT_RISING"/>
                <Table Key="下降沿事件" Value="GPIO_MODE_EVT_FALLING"/>
                <Table Key="上升沿下降沿事件" Value="GPIO_MODE_EVT_RISING_FALLING"/>                
            </Mode>
            <Mode Name="速度" Widgets="QComboBox">
                <Table Key="低速" Value="GPIO_SPEED_FREQ_LOW"/>
                <Table Key="中速" Value="GPIO_SPEED_FREQ_MEDIUM"/>
                <Table Key="高速" Value="GPIO_SPEED_FREQ_HIGH"/>             
            </Mode>
            <Mode Name="上下拉" Widgets="QComboBox">
                <Table Key="上拉" Value="GPIO_NOPULL"/>
                <Table Key="下拉" Value="GPIO_PULLUP"/>
                <Table Key="不上拉不下拉" Value="GPIO_PULLDOWN"/>             
            </Mode>
            <UserLabel Name="用户标签" Widget="QLineEdit"/>
        </Configuration>
    </SubFamily>
</IP>
